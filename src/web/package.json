{
  "name": "refactortrack-web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageThreshold={\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}",
    "lint": "eslint src --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "typecheck": "tsc --noEmit",
    "analyze": "vite build --analyze",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.0.0",
    "@auth0/auth0-spa-js": "^2.1.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fingerprintjs/fingerprintjs": "^3.4.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/x-data-grid": "^6.0.0",
    "@mui/x-date-pickers": "^6.0.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@sentry/browser": "^7.0.0",
    "@tanstack/react-virtual": "^3.0.0",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^6.0.0",
    "io-ts": "^2.2.20",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "opossum": "^6.0.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.0",
    "react-intl": "^6.0.0",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.14.0",
    "recharts": "^2.7.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7",
    "validator": "^13.9.0",
    "yup": "^1.2.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.7.0",
    "@jest/globals": "^29.6.0",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-legacy": "^4.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.63.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.0",
    "vite": "^4.4.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.scss": [
      "prettier --write"
    ]
  }
}